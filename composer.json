{
    "name": "inweb/admin",
    "description": "Admin Panel for Laravel Framework",
    "version": "1.0.0",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "admin",
        "admin panel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.7",
        "inwebcomp/translations-parser": "dev-master",
        "spatie/once": "^2.0",
        "laravel/scout": "^7.1",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Admin\\Tests\\": "tests/",
            "InWeb\\Admin\\Database\\Seeds\\": "src/database/seeds/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Admin\\App\\Providers\\AdminCoreServiceProvider"
            ]
        }
    }
}
