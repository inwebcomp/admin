{
    "name": "inweb/admin",
    "description": "Admin Panel for Laravel Framework",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "admin",
        "admin panel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "laravel/scout": "^8.6",
        "spatie/laravel-permission": "^5.4",
        "cakephp/chronos": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Admin\\": "src/",
            "InWeb\\Admin\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Admin\\Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Admin\\App\\Providers\\AdminCoreServiceProvider"
            ]
        }
    }
}
